{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": "off",
        "no-plusplus": "off",
        "import/no-unresolved": "off",
        "no-param-reassign": "off",
        "no-unused-expressions": "off"
    }
}
